FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      turbulenceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType RAS;

RAS
{
    RASModel        ML_SA_CG; //kOmegaSST; // kEpsilon;

    turbulence      on;

    printCoeffs     on;

    widthCoeff      4.0; // Incase you use Laplace filter

    hh_             1.9;
}


